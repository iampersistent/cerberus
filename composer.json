{
    "name": "picr/cerberus",
    "description": "A guard for actions and data",
    "keywords": ["permissions"],
    "license": "MIT",
    "require": {
        "marc-mabe/php-enum": "^2.2",
        "php": ">=7.0.0",
        "php-ds/php-ds": "^1.1",
        "flow/jsonpath": "^0.3.4"
    },
    "require-dev": {
        "codeception/codeception": "2.2.*",
        "codeception/aspect-mock": "*"
    },
    "autoload": {
        "psr-4": {
            "Cerberus\\": "src/",
            "Test\\Unit\\": "tests/unit",
            "Test\\Functional\\": "tests/functional",
            "Test\\Acceptance\\": "tests/acceptance",
            "TestData\\": "tests/_data/fixtures"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "dev"
}
